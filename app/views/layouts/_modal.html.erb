<!-- モーダル・ダイアログ -->
<div class="modal fade" id="start_pop" tabindex="-1">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title"><%= @contest.title %> START!!</h4>
			</div>
			<div class="modal-body">
			コンテスト: <%= @contest.title %> の開始時間です
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal" id="start_button">閉じる</button>
			</div>
		</div>
	</div>
</div>


<div class="modal fade" id="end_pop" tabindex="-1">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title"><%= @contest.title %> END!!</h4>
			</div>
			<div class="modal-body">
				コンテスト: <%= @contest.title %> の終了時間です
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal" id="end_button">閉じる</button>
			</div>
		</div>
	</div>
</div>

