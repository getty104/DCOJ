<table class="table table-striped">
  <thead>
    <tr>
      <th><%= :number %></th>
      <th><%= :title %></th>
      <th><%= :created_by %></th>
      <th><%= :accepted_by %></th>
      <th>level</th>
    </tr>
  </thead>
  <tbody>
    <% questions.each do |question| %>
      <tr>
        <td><%=  question.id %></td>
        <td><%= question.title %></td>
        <td><%= question.created_user.name %></td>
        <td><%= question.users.length %></td>
         <td><%= question.question_level.to_i %></td>
        <td><%=link_to "詳細",question, :class => 'btn btn-primary' %></td>
      </tr>
    <% end %>
  </tbody>
  <%= paginate questions, param_name: level_page %>
</table>