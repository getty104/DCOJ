<h2 class="text-center"><%= title %></h2>
<table class="table table-striped">
  <tbody>
  <% number = 1 %>
    <% users.each do |user| %>
    <tr class="text-center">
    <td><%= number %></td>
      <td><%= link_to user.name, user, style: "color:green;"  %></td>
    </tr>
    <% number += 1 %>
    <% if number == 6 %>
    <% break %>
    <% end %>
    <% end %>
  </tbody>
</table> 

