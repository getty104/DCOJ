
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= :number %></th>
      <th><%= :title %></th>
      <th><%= :created_by %></th>
      <th><%= :accepted_by %></th>
      <th>level</th>
    </tr>
  </thead>
  <tbody>
    <% questions.each do |question| %>
    <tr>
      <td><%=  question.id %></td>
      <td><%= link_to question.title, question, style: "color:green;"  %></td>
      <td><%= link_to question.created_user.name,question.created_user, style: "color:green;"  %></td>
      <td><%= question.users.length %></td>
      <td><%= question.question_level.to_i %></td>
    </tr>
    <% end %>
  </tbody>
    <%= paginate questions, param_name: level_page, remote: true %>
</table>  


